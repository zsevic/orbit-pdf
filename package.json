{
  "name": "@zsevic/orbit-pdf",
  "description": "CLI tool for converting Orbit HTML resume to pdf",
  "version": "1.1.5",
  "bin": {
    "orbit-pdf": "./bin/run"
  },
  "bugs": "https://github.com/zsevic/orbit-pdf/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "chai": "^4.3.3",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "husky": "^5.1.3",
    "mocha": "^10.1.0",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chalk": "^2.4.2",
    "puppeteer": "^8.0.0",
    "validator": "^13.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/zsevic/orbit-pdf",
  "husky": {
    "hooks": {
      "pre-commit": "npm run version"
    }
  },
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "orbit-pdf",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "postpublish": "npm run publish-npm",
    "publish-npm": "npm publish --access public --ignore-scripts --@zsevic:registry='https://registry.npmjs.org'",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "posttest": "npm run lint -- --fix",
    "update-readme": "oclif-dev readme && git add README.md"
  }
}
